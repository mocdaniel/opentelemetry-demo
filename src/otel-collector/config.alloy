remote.s3 "s3_config" {
  path = "s3://alloy-cfg/config.alloy"

  client {
    key      = sys.env("ALLOY_S3_ACCESS_KEY")
    secret   = sys.env("ALLOY_S3_SECRET_KEY")
    endpoint = "https://storage.netways.cloud"
    region   = "dummy"
  }
}

import.string "imported_config" {
  content = remote.s3.s3_config.content
}

imported_config.full_config "actual_config" {}
